{
    "name": "seboettg/forest",
    "description": "Tree data structures for PHP – containing general tree, binary tree, and AVL tree",
    "type": "library",
    "keywords": [
        "avl",
        "avl-tree",
        "avltree",
        "binary-search-tree",
        "search-trees",
        "tree-structure",
        "datastructures",
        "datastructures-algorithms",
        "algorithms",
        "visitor-pattern"
    ],
    "autoload": {
        "psr-4": {
            "Seboettg\\Forest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seboettg\\Forest\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "seboettg/collection": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Böttger",
            "email": "seboettg@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml"
    }
}
